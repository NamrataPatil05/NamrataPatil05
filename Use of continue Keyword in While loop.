using System;
using System.IO;
using System.Linq;
using System.Collections.Generic;

namespace CSharp_Shell
{

    public class Program 
    {
        public static void Main()
        {
        	int i=1;
        	while(i<=10)
			{
				i++;
				if(i%2==0)
				{
				
				if(i>=6 && i<=10)
				{
					Console.WriteLine(i);
					continue;
					
				}
				
				}
			}
        }
    }
}

Output:-
6
8
10
